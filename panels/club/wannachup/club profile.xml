<panel>
  <layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true"/>
  <container class="wannachup_roundsemi">
    <layout class="arrange_vertical_attachment" layout="50,40,1,-1,5" offset="0" gap="8"/>
    <layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="0"/>
    <!--Name-->
    <container>
      <layout class="arrange_vertical_attachment" alignment="fill" offset="0" gap="0"/>
      <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>
      <widget class="text" font="title" colour="text" multiline="true" id="titl" size="14" alignment="centre">
        <record id="object_property">
          <integer id="get_property" value="titl"/>
        </record>
      </widget>
    </container>
    <container>
      <attachment_group class="horizontal_arrange" horizontal_gap="4" horizontal_layout="-1,-1"/>
      <!--nickname-->
      <container>
        <layout class="vertical_arrange" vertical_layout="-1,-1" offset="0" gap="0"/>
        <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>
        <widget class="label" id="TCnL" font="label" alignment="centre">
          <record id="object_property">
            <integer id="get_property" value="TCnn"/>
            <boolean id="name" value="true"/>
          </record>
        </widget>
        <widget class="text" font="label" colour="text" id="TCnV" size="12" alignment="centre">
          <record id="object_property">
            <integer id="get_property" value="TCnn"/>
          </record>
        </widget>
      </container>
      <!--year-->
      <container>
        <layout class="vertical_arrange" vertical_layout="-1,-1" offset="0" gap="0"/>
        <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>
        <widget class="label" id="yrfL" font="label" alignment="centre">
          <record id="object_property">
            <integer id="get_property" value="Cyfn"/>
            <boolean id="name" value="true"/>
          </record>
        </widget>
        <widget class="dashed_number_label" font="label" colour="text" id="yrfd" size="12" alignment="centre">
          <boolean id="NumF" value="0"/>
          <record id="object_property">
            <integer id="get_property" value="Cyfn"/>
          </record>
        </widget>
      </container>
    </container>
    <container/>
    <container>
      <layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="0"/>
      <widget class="team_container_information_background_panel" id="wtbp" priority="1">
        <record id="object_property" get_property="objt" set_property="objt"/>
      </widget>
    </container>
    <container/>
  </container>
</panel>
